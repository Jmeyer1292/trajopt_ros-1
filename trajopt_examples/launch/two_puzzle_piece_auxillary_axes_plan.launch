<?xml version="1.0"?>
<launch>
  <!-- By default we do not overwrite the URDF. Change the following to true to change the default behavior -->
  <!-- The name of the parameter under which the URDF is loaded -->
  <arg name="robot_description" default="robot_description"/>
  <arg name="plotting" default="true"/>

  <!-- Load universal robot description format (URDF) -->
  <param name="$(arg robot_description)" command="$(find xacro)/xacro --inorder '$(find trajopt_examples)/urdf/two_robots_puzzle_piece_workcell.xacro'" />

  <!-- The semantic description that corresponds to the URDF -->
  <param name="$(arg robot_description)_semantic" textfile="$(find trajopt_examples)/config/two_puzzle_piece_workcell.srdf" />

  <node pkg="trajopt_examples" type="trajopt_examples_two_robots" name="trajopt_examples_two_robots" output="screen">
    <param name="plotting" type="bool" value="$(arg plotting)"/>
  </node>
</launch>
